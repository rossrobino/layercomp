{
	"name": "uico",
	"version": "0.1.6",
	"description": "UI components for TailwindCSS projects",
	"keywords": [
		"CSS",
		"TailwindCSS",
		"components"
	],
	"homepage": "https://uico.robino.dev",
	"license": "MIT",
	"author": {
		"name": "Ross Robino",
		"url": "https://robino.dev"
	},
	"repository": "github:rossrobino/uico",
	"type": "module",
	"types": "./plugin/index.d.ts",
	"main": "./plugin/index.js",
	"exports": {
		".": {
			"types": "./plugin/index.d.ts",
			"default": "./plugin/index.js"
		}
	},
	"files": [
		"plugin"
	],
	"scripts": {
		"dev": "vite dev",
		"host": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --write . --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss",
		"pub": "bun i && bun format && bun doc && publint && npm publish --access public",
		"doc": "bun scripts/copyReadMe.js"
	},
	"dependencies": {
		"tailwindcss": "^3.3.5"
	},
	"devDependencies": {
		"@sveltejs/adapter-vercel": "^3.0.3",
		"@sveltejs/kit": "^1.27.5",
		"@tailwindcss/typography": "^0.5.10",
		"@types/node": "^20.9.0",
		"@typescript-eslint/eslint-plugin": "^6.11.0",
		"@typescript-eslint/parser": "^6.11.0",
		"autoprefixer": "^10.4.16",
		"drab": "^4.1.4",
		"eslint": "^8.53.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.35.0",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.0",
		"prettier-plugin-tailwindcss": "^0.5.7",
		"publint": "^0.2.5",
		"robino": "^0.0.39",
		"svelte": "^4.2.3",
		"svelte-check": "^3.6.0",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.5.0"
	}
}
